@using Amard.Shahrsazi.Core.Utilities
@using AmardWebShahrsazi.Models.Enum
@model CompanyPosType?

@(Html.Kendo().ComboBox().Name("CompanyPosType")
    .Filter(FilterType.Contains)
    .BindTo(Enum.GetValues(typeof(CompanyPosType)).Cast<CompanyPosType>().Select(v => new SelectListItem {
        Text = v.GetDisplayName(),
        Value = ((int)v).ToString()
    }).ToList()))
